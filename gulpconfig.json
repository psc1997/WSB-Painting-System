{
    "paths": {
        "root": "site",
        "src": "site/resources/_src",
        "dest": "site/public/dist",
        "styles": {
            "src": "site/resources/_src/scss",
            "dest": "site/public/dist/css"
        },
        "scripts": {
            "src": "site/resources/_src/js",
            "dest": "site/public/dist/js"
        },
        "fonts": {
            "src": "site/resources/_src/fonts",
            "dest": "site/public/dist/fonts"
        },
        "iconfont": {
            "src": "site/resources/_src/iconfont",
            "dest": "site/public/dist/iconfont"
        },
        "images": {
            "src": "site/resources/_src/img",
            "dest": "site/public/dist/img"
        },
        "videos": {
            "src": "site/resources/_src/video",
            "dest": "site/public/dist/video"
        }
    }
}
